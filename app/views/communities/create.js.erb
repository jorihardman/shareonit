<% if @community.errors.any? %>
  $('#new_community_form').html('<%= escape_javascript(render 'communities/form') %>');
  resize_fancybox();
<% else %>
  $.fancybox.close();
  $('#notice').html('Community added.');
  $('#my_communities tr:first').after('<%= escape_javascript(community_table_row @community) %>');
  $('.buttonset').buttonset();
<% end %>
