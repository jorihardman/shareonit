<% if @community.errors.any? %>
  $('#new_community_form').html('<%= j render('communities/form') %>');
  resize_fancybox();
<% else %>
  $.fancybox.close();
  $('#notice').html('Community added.');
  $('#my_communities tr:first').after('<%= j community_table_row(@community) %>');
  $('.buttonset').buttonset();
<% end %>
